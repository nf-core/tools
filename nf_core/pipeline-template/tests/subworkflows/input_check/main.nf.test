nextflow_workflow {

    name "Test Workflow INPUT_CHECK"
    script "subworkflows/local/input_check.nf"
    workflow "INPUT_CHECK"

    test("Should run without failure") {
        when {
            params {
                outdir = "tests/results"
                outdir = "$outputDir"
            }
        }

        then {
            assert workflow.success
        }
    }

}
